@model JobSeekers

<div class="container">
    <div class="row">
        <div class="col-md-4 text-center">
            <img src="~/@Model.profileImage" alt="Profile Image" class="rounded-circle" width="100" height="100">
            <h5 class="mt-3">@Model.FullName</h5>
            <p>@Model.Email</p>
        </div>
        <div class="col-md-8">
            <h6 class="mb-3">Contact Information</h6>
            <p><strong>Phone:</strong> @Model.ContactNo</p>
            <p><strong>Email:</strong> @Model.Email</p>
            <h6 class="mb-3">About</h6>
            <p>@Model.About</p>
            <h6 class="mb-3">Address</h6>
            <p>@Model.Address</p>
        </div>
    </div>
</div>
